<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id='@+id/emergency_number_layout'
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/numbersListView"
        android:layout_width="@dimen/emergency_nb_width"
        android:layout_height="@dimen/emergency_nb_height"
        android:layout_marginTop="@dimen/emergency_nb_margin_top"
        android:layout_marginBottom="@dimen/emergency_nb_margin_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </ListView>

</androidx.constraintlayout.widget.ConstraintLayout>
